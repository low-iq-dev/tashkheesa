<%
  const layoutVariant = (typeof layout !== 'undefined' && layout) ? String(layout) : 'public';
%>
<% if (layoutVariant === 'portal') { %>
  <%- include('../layouts/portal') %>
<% } else if (layoutVariant === 'auth') { %>
  <%- include('../layouts/auth') %>
<% } else { %>
  <%- include('../layouts/public') %>
<% } %>
